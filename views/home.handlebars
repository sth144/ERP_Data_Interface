<!doctype html>
<html>
  <body onload="">

	<h1 id="model-header" class="text-center" display="none">{{model}}</h1>

  	<div class="row">
  		<div class="col-md-2 align-top">
	  		<div id="tableForms"></div>
	  	</div>
	  	<div class="col-md-8 align-top">

		    <table class="table table-striped table-bordered" id="tab">
		    	<thead>
		    		<!-- Dynamic column headers using number of keys in result -->
		    		{{#if results.[0]}}
		 		   		{{#with results.[0]}}
		 		   			{{#each this}}
		 		   				<th>{{@key}}</th>
		 		   			{{/each}}
		 		   		{{/with}}
		 		   	{{/if}}
		    	</thead>
			    <tbody>
			      {{#each results}}
			      	<tr>
			      		{{#each this}}
			      			<td>{{this}}</td>
			      		{{/each}}
			      	</tr>
			      {{/each}}
			    </tbody>
			 </table>
			 <h4>{{intro}}</h4>

	   	</div>
	   	<div class="col-md-2 align-top">
	  		<div id="tableForms2"></div>
	  	</div>
	</div>
	<div id="bottom-button"></div>
  </body>
</html>
